<% if (data.length > 0) { %>
<% for (var i = 0; i < data.length; i++) { %>
<div class="panel panel-default">
    <div class="panel-heading">
        <h4 class="panel-title">
            <%= data[i].name %>
        </h4>
    </div>
    <div class="panel-body">
        <p>
            <label>Tipo de punto de contacto:</label>
            <% if (data[i].type == '') { %> <% } %>
            <% if (data[i].type == 'contactPoint') { %> Punto de contacto <% } %>
            <% if (data[i].type == 'attorney') { %> Abogado <% } %>
        </p>

        <p>
            <label>Teléfono:</label>
            <%= data[i].telephone %>
        </p>

        <button class="btn btn-default" data-action="delete_contactpoint" data-id="<%= data[i].id %>">Eliminar</button>
        <button class="btn btn-default" data-action="edit_contactpoint" data-id="<%= data[i].id %>" data-show="modal" data-target="#genericModal">Editar</button>
    </div>
</div>
<% } %>
<% } else { %>
<div class="alert alert-warning" role="alert"><strong>Atención:</strong> No se han registrado puntos de contacto adicional</div>
<% } %>