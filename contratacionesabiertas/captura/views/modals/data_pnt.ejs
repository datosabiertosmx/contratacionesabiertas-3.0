<form id="dataPNT_form">
    <input name="id" value="<%= id %>" hidden>
    <input name="contractingprocess_id" value="<%= contractingprocess_id %>" hidden>
    <p><label title='' >
    Ejercicio*</label>
        <input name="fiscalYear" class="form-control" value="<%= fiscalyear %>" required></p>
    <p><label title=''>
    Fecha de inicio del periodo que se reporta*</label>
        <input type="text" class="form-control" id= "pnt_date1" name = "reportingPeriodStartDate" value="<%= reportingperiodstartdate %>" required>
    <p><label title=''>
    Fecha de término del periodo que se reporta*</label>
        <input type="text" class="form-control" id= "pnt_date1" name = "reportingPeriodEndDate" value="<%= reportingperiodenddate %>" required>
    <p><label title=''>
    Area(s) responsable(s) que genera(n) posee(n) actualiza(n) y publica(n) los datos*</label>
        <input type="text" name="dataResponsibleunit" class="form-control" minlength="1" maxlength="4000" value="<%= dataresponsibleunit %>" required></p>
    <p><label title=''>
    Fecha de validación*</label>
        <input type="text" class="form-control" id= "pnt_date3" name = "valitationDate" value="<%= valitationdate %>" required>
    <p>
    <p><label title=''>
    Fecha de actualización*</label>
        <input type="text" class="form-control" id= "pnt_date4" name = "updateDate" value="<%= updatedate %>" required>
    <p><label title=''>
    Notas*</label>
        <textarea class="form-control" rows="5" name = "notes" value="<%= notes %>" required><%=(typeof notes!= "undefined")? notes : ''%></textarea>
    <p>
        <button type="submit" class="btn btn-default">Guardar</button>
    </p>
</form>